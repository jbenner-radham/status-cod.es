{
  "name": "status-cod.es",
  "version": "0.0.0",
  "private": true,
  "description": "An HTTP status codes reference site.",
  "homepage": "https://github.com/jbenner-radham/status-cod.es#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/status-cod.es/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/status-cod.es.git"
  },
  "license": "MIT",
  "author": "James Benner <james.benner@gmail.com> (https://www.jamesbenner.com/)",
  "scripts": {
    "build": "vite build",
    "build-status-codes": "node ./scripts/build-status-codes.cjs",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test:unit": "vitest --environment jsdom --root src/"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "debracketify": "^0.1.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@radioactivehamster/date-time": "^0.1.5",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.1.0",
    "cypress": "^10.10.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "gh-pages": "^4.0.0",
    "http-status-code-registry": "^0.2.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "sort-package-json": "^2.1.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^3.1.8",
    "vite-plugin-html": "^3.2.0",
    "vitest": "^0.24.3"
  }
}
